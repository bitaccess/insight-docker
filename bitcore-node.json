{
  "network": "mainnet",
  "port": 3001,
  "services": [
    "bitcoind",
    "web",
    "insight-api",
    "insight-ui"
  ],
  "servicesConfig": {
    "insightConfig": {
      "apiPrefix": "insight-api",
      "routePrefix": ""
    },
    "bitcoind": {
      "spawn": {
        "datadir": "/bitcoin",
        "network": "mainnet",
        "exec": "/usr/lib/node_modules/bitcore/node_modules/bitcore-node/bin/bitcoind"
      }
    }
 }
}
